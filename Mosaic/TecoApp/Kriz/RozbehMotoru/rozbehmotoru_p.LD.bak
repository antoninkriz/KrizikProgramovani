PROGRAM rozbehmotoru_p
  VAR_INPUT
  END_VAR
  VAR_OUTPUT
  END_VAR
  VAR
    casovac : TON;
    pom : BOOL;
    cas : TIME;
    zarovka : BOOL;
  END_VAR
  VAR_TEMP
  END_VAR
{NOCODE}
(*
  Komentáø ke schématu
*)

{NETWORK}
(*
Start + Spinani tranzistoru T1
*)
  LD_BOOL   "start"
  OR        "t1"
  ANDN      "stop"
  ST        "t1"
{/NETWORK}

{NETWORK}
(*
Generovani casoveho zpozdeni po startu
*)
  LD_BOOL   "t1"
  {POU:TON} "casovac"
    {PIN:IN} {/PIN}
    {PIN:PT} "T#5s" {/PIN}
    {PINO:Q} {/PIN}
    {PINO:ET} "cas" {/PIN}
  {/POU}
  ST        "pom"
{/NETWORK}

{NETWORK}
(*
Spinani tranzistoru T2
*)
  LD_BOOL   "pom"
  ST        "t2"
{/NETWORK}


{/NOCODE}
{ILCODE}
(* Network 1 *)
(* 23 *)    LD_BOOL   start
(* 24 *)    OR        t1
(* 25 *)    ANDN      stop
(* 26 *)    ST        t1
(* Network 2 *)
(* 33 *)    LD_BOOL   t1
(* 35 *)    ST        casovac.IN
(* 34 *)    CAL casovac (PT:=T#5s)
(* 38 *)    LD        casovac.ET
(* 38 *)    ST        cas
(* 37 *)    LD        casovac.Q
(* 40 *)    ST        pom
(* Network 3 *)
(* 47 *)    LD_BOOL   pom
(* 48 *)    ST        t2
END_PROGRAM
{STCODE}

